
<h1 class="list-title">Completed Tasks</h1>
</br>
<div class="container">
  <table class="table table-bordered table-striped">
    <thead>
      <tr>
        <th>Name</th>
        <th>Start on</th>
        <th>Due on</th>
        <th>Completed</th>
        <th colspan="3"></th>
      </tr>
    </thead>
    <tbody id="sortable">
      <div id='all-tasks'><%= render @tasks %></div>
    </tbody>
  </table>
</div>
<br>
<div class="bottom-links">
  <%= link_to 'Back', root_path, class: "btn btn-primary active" %>
</div>
