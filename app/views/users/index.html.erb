
<h1>Listing Users</h1>

<table>
  <thead>
    <tr>
      <th>Email</th>
      <th>Password digest</th>
      <th>Name</th>
      <th>Confirmation token</th>
      <th>Remember token</th>
      <th>Uid</th>
      <th>Token</th>
      <th>Pid</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @users.each do |user| %>
      <tr>
        <td><%= user.email %></td>
        <td><%= user.password_digest %></td>
        <td><%= user.name %></td>
        <td><%= user.confirmation_token %></td>
        <td><%= user.remember_token %></td>
        <td><%= user.uid %></td>
        <td><%= user.token %></td>
        <td><%= user.pid %></td>
        <td><%= link_to 'Show', user %></td>
        <td><%= link_to 'Edit', edit_user_path(user) %></td>
        <td><%= link_to 'Destroy', user, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New User', new_user_path %>
